<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.org.shelly.edu.mapper.ClassStudentMapper">

    <resultMap id="BaseResultMap" type="cn.org.shelly.edu.model.pojo.ClassStudent">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="sex" column="sex" jdbcType="TINYINT"/>
            <result property="universityName" column="university_name" jdbcType="VARCHAR"/>
            <result property="departmentName" column="department_name" jdbcType="VARCHAR"/>
            <result property="major" column="major" jdbcType="VARCHAR"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="gmtModified" column="gmt_modified" jdbcType="TIMESTAMP"/>
            <result property="cid" column="cid" jdbcType="BIGINT"/>
            <result property="isDeleted" column="Is_deleted" jdbcType="TINYINT"/>
            <result property="sno" column="sno" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,sex,
        university_name,department_name,major,
        gmt_create,gmt_modified,cid,
        Is_deleted,sno
    </sql>
</mapper>
